Private Sub ToggleButton1_Click()
    Dim SearchValue As String
    Dim SearchSheet As Worksheet
    Dim SearchColumn As Range
    Dim RetrieveColumn As Range
    Dim NewDataLocation As Worksheet
    Dim Cell As Range
    
    'Assign text box values to variables
    SearchValue = TextBoxValue.Text
    Set SearchSheet = ThisWorkbook.Sheets(TextBoxSearchSheet.Text)
    Set SearchColumn = SearchSheet.Columns(TextBoxSearchColumn.Text)
    Set RetrieveColumn = SearchSheet.Columns(TextBoxRetrieveColumn.Text)
    Set NewDataLocation = ThisWorkbook.Sheets(TextBoxNewDataLocation.Text)
    
    'Loop through each cell in the search column
    For Each Cell In SearchColumn
        'If the cell contains the search value, copy the corresponding cell from the retrieve column to the new data location
        If InStr(Cell.Value, SearchValue) > 0 Then
            RetrieveColumn.Cells(Cell.Row).Copy Destination:=NewDataLocation.Cells(Cell.Row)
        End If
    Next Cell
End Sub


=FILTER(Sheet1!A:Z, MMULT(--ISNUMBER(SEARCH(TRANSPOSE(Sheet2!C:C), Sheet1!K:K)), SEQUENCE(ROWS(Sheet2!C:C),,,0))>0, "No results")
